t=int(input())
while t>0:
    t-=1    
    n=int(input())
    a=list(int(i) for i in input().split())
    b=list(int(i) for i in input().split())
    a.sort()
    b.sort()
    check=True
    for i in range(0, len(a)):
        if a[i]>b[i]:
            check=False
    if check==True:
        print("YES")
    else:
        print("NO")
